<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="25.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.demo11.OtherLabs">
    <padding>
        <Insets bottom="40.0" left="40.0" right="40.0" top="40.0" />
    </padding>
    <children>
        <Label text="Виберіть лабораторну роботу:" style="-fx-font-size: 20px; -fx-font-weight: bold; -fx-text-fill: #2d4059;" />
        <VBox spacing="15.0" alignment="CENTER">
            <Button fx:id="lab5Button" mnemonicParsing="false" onAction="#openLab5" prefHeight="50.0" prefWidth="250.0" text="Лабораторна 5" />
            <Button fx:id="lab6Button" mnemonicParsing="false" onAction="#openLab6" prefHeight="50.0" prefWidth="250.0" text="Лабораторна 6" />
            <Button fx:id="lab7Button" mnemonicParsing="false" onAction="#openLab7" prefHeight="50.0" prefWidth="250.0" text="Лабораторна 7" />
            <Button fx:id="lab8Button" mnemonicParsing="false" onAction="#openLab8" prefHeight="50.0" prefWidth="250.0" text="Лабораторна 8" />
        </VBox>
    </children>
</VBox>
