<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.media.MediaView?>

<VBox spacing="10" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.demo11.MediaController">
    <padding>
        <Insets top="10" right="10" bottom="10" left="10"/>
    </padding>
    <MediaView fx:id="mediaView" fitHeight="450" fitWidth="1100" preserveRatio="true" />
    <HBox spacing="10">
        <Button fx:id="playButton" text="Play" onAction="#playButton_method" />
        <Button fx:id="pauseButton" text="Pause" onAction="#pauseButton_method" />
        <Button fx:id="resetButton" text="Reset" onAction="#resetButton_method" />
    </HBox>
</VBox>

